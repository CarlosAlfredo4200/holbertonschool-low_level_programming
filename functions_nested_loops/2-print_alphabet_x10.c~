/* betty style doc for function main goes there */
/**
 *print_alphabet - unction that prints the alphabet, in lowercase
 *
 *Return: Always 0 (success)
 */

void print_alphabet_x10(void)
{
    int c;
    for (int i = 0; i < 10; i++)
    {
	for (c = 'a'; c <= 'z'; c++)
	{
	    putchar(c);
	}
	putchar('\n');
    }
}
