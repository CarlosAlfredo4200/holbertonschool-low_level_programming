#include "main.h"
/**
 * print_rev - a function that prints a string, in reverse, followed by a new
 *
 * @s: This is char
 *
 */
void rev_string(char *s)
{
int k, i;
int len = 0;
char aux;
char *ptr = s;
for (k = 0; s[k] != '\0'; k++)
{
len++;
}

char aux[len + 1];
for (i = len - 1; i >= 0; i--)
{
aux[len - 1 - i] = *(ptr + i);
}
}
