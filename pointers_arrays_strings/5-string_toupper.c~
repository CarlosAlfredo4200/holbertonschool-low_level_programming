#include "main.h"
/**
 * string_toupper - Write a function that changes all lowercase letters of a
 * string to uppercase.
 *
 * @hi: This is the input string
 *
 * Return: Lowercase changed to uppercase of the string
 */
char *string_toupper(char *cadena)
{
int k = 0;
int cambiar = 'a' - 'A';
for (k ; cadena[k] != '\0'; ++k)
{
if (cadena[k] >= 'a' && cadena[k] <= 'z')
{
cadena[k] = cadena[k] - cambiar;
}
}
return (cadena);
}
